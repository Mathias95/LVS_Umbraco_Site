@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Frontpage>
@using ContentModels = Umbraco.Web.PublishedContentModels;


<!-- CTA Section begins -->
<section class="cards">
    <div class="cards-container">
        <h2>Hvad tilbyder vi som værested</h2>
        <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed.</p>
        <div class="card-wrapper">
            @foreach (CtaBox b in Model.Content.Services)
            {
                <div class="card-box wrapper y10">
                    <a href="@b.BoxLink.Url"><img src="@b.BoxPicture.Url" alt=""></a>
                    <div class="card-body">
                        <h3>@b.Headline</h3>
                        <p>@b.BoxText</p>
                        <a href="@b.BoxLink.Url">Læs Mere <i class="fas fa-arrow-right"></i></a>
                    </div>
                </div>
            }
        </div>
    </div>
</section>
